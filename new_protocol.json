{
    "RIP": [
        "conf ter",
        "router rip",
        "version 2",
        "network {network}",
        "network 10.0.5.0",
        "redistribute ospf 1",
        "redistribute eigrp 1",
        "no auto-summary"
    ],
    "OSPF": [
        "conf ter",
        "router ospf 1",
        "network {network} area 0",
        "network {network} area 0",
        "network {network} area 0",
        "redistribute rip subnets",
        "redistribute eigrp 1 subnets"
    ],
    "EIGRP": [
        "conf ter",
        "router eigrp 1",
        "network 10.0.5.0",
        "network {network}",
        "redistribute ospf 1",
        "redistribute rip" 
    ]
}
